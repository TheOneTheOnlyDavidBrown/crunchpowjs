{
  "name": "liaisonjs",
  "version": "1.0.1",
  "homepage": "https://github.com/TheOneTheOnlyDavidBrown/liaisonjs",
  "authors": [
    "David Brown <dbrown@gmail.com>"
  ],
  "description": "ES6/2015 library that allows for passing of view data to ES6/2015 modules with two way data binding and model validation against a schema",
  "main": [
    "src/Liaison.js",
    "src/BaseModel.js",
    "src/Binding.js",
    "src/Router.js"
  ],
  "moduleType": [
    "es6"
  ],
  "keywords": [
    "es6",
    "data-binding",
    "model",
    "component",
    "loading"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "fetch": "~0.9.0"
  }
}
